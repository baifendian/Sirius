  GROUP_CACHE_TIMEOUT: k8s_LDAP_GROUP_CACHE_TIMEOUT
  USER_ATTR_MAP:
    username: k8s_LDAP_USER_ATTR_MAP_USERNAME
    password: k8s_LDAP_USER_ATTR_MAP_PASSWORD

