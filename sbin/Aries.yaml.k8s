REST_BASE_URI: k8s_REST_BASE_URI

DATABASES:
  default:
    ENGINE: k8s_DATABASES_DEFAULT_ENGINE
    NAME: k8s_DATABASES_DEFAULT_NAME
    HOST: k8s_DATABASES_DEFAULT_HOST
    PORT: k8s_DATABASES_DEFAULT_PORT
    USER: k8s_DATABASES_DEFAULT_USER
    PASSWORD: k8s_DATABASES_DEFAULT_PASSWORD
  kd_agent_bdms:
    ENGINE: k8s_DATABASES_BDMS_ENGINE # Add 'postgresql_psycopg2', 'mysql', 'sqlite3' or 'oracle'.
    NAME: k8s_DATABASES_BDMS_NAME        # Or path to database file if using sqlite3.
    USER: k8s_DATABASES_BDMS_USER    # Not used with sqlite3.
    PASSWORD: k8s_DATABASES_BDMS_PASSWORD                  # Not used with sqlite3.
    HOST: k8s_DATABASES_BDMS_HOST             # Set to empty string for localhost. Not used with sqlite3.
    PORT: k8s_DATABASES_BDMS_PORT                       # Set to empty string for default. Not used with sqlite3.

